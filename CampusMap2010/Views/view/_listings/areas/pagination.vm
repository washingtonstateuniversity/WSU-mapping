#if(!$target)
#set($target = $status.title)
#end

<div class="pagination $Pagination_location ui-widget-content ui-corner-all">
    <h6>Showing $list_items.FirstItemIndex - $list_items.LastItemIndex of $list_items.TotalItems</h6>
   #if($list_items.HasPreviousPage || $list_items.HasNextPage)
	   <ul class=" ui-tabs-nav ui-helper-reset ui-helper-clearfix ui-widget-header ui-corner-bottom">
			<li>
				#if($list_items.HasPreviousPage)
					$PaginationHelper.CreatePageLink( 1, "first",null, "%{searchId=$!searchId,target=$target}" )
				#else
					<a href="#">first</a>
				#end
			</li>
			<li>
				#if($list_items.HasPreviousPage)
					$PaginationHelper.CreatePageLink( $list_items.PreviousPageIndex, "prev",null, "%{searchId=$!searchId,target=$target}" )
				#else
					<a href="#">prev</a>
				#end
			</li>
			<li>
				#if($list_items.HasNextPage)
					$PaginationHelper.CreatePageLink( $list_items.NextPageIndex, "next",null, "%{searchId=$!searchId,target=$target}" )
				#else
					<a href="#">next</a>
				#end
			</li>
			<li>
				#if($list_items.HasNextPage)
					$PaginationHelper.CreatePageLink( $list_items.TotalPages, "last",null, "%{searchId=$!searchId,target=$target}" )
				#else
					<a href="#">last </a>
				#end
			</li>
		</ul>
	#end
</div>

