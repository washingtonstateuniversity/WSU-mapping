
<input type="hidden" name="child_geos[$idx].id" value="$!child_item.id" data-child_id="$!child_item.id"/>
<input type="hidden" name="child_geos[$idx].geom_type" value="#if($child_item.default_type.id=='2' || $selectedType==2)polyline#else#if($child_item.default_type.id=='3' || $selectedType==3)polygon#end#end" />
<input type="hidden" name="child_geos[$idx].name" value="$!child_item.name"/>
#set($styleChoice = "")
#capturefor(styleChoice)
	#if($ele.type!='')
		#foreach($style in $styles)
			#if($child_item.style.Contains($style))
				$style.id
			#end
		#end
	#end
#end
<input type="hidden" name="child_geos[$idx].style[]" value="$styleChoice"/>



#set($spatialpart = $child_item.latlongs())
<h5>Latitudes and Longitudes:</h5>
	#if($spatialpart)
	<!--$spatial ||
	$real_sp_type-->
		#if($spatialpart.PropertyType().IsArray)
			#set($i = 0)
			#foreach($item in $!spatialpart )
				<textarea name="child_geos[$idx].boundary[$i]" style="width:100%;height:250px;" id="latLong" cols="80" rows="25">$!item</textarea>
				#set($i = $i +1)
			#end
		#else
			<textarea name="child_geos[$idx].boundary[0]" style="width:100%;height:250px;" id="latLong" cols="80" rows="25">$!spatialpart</textarea>
		#end
	#else
		<textarea name="child_geos[$idx].boundary[0]" style="width:100%;height:250px;" id="latLong" cols="80" rows="25">$!spatialpart</textarea>
	#end
<h5>Encoded:</h5>
	<textarea name="child_geos[$idx].encoded" id="geometric_encoded" class="ui-widget ui-widget-content ui-corner-all" style="width:100%;height:50px;" cols="80"  rows="5">$!child_item.encoded</textarea>