<p>
<a href="New_usertags.castle">Create tags</a>
</p>
<table width="60%" border="0" style="float:right;margin-right:5px;margin-top:7px;">
  <tr>
    <td>Showing $usertags.FirstItem - $usertags.LastItem of $usertags.TotalItems</td>
    <td align="right">
      #if($usertags.HasPrevious) $usertags.CreatePageLink( 1, "first",null, "%{searchId=$!searchId,status='draft'}" )  #end
      #if(!$usertags.HasPrevious) first #end
      #if($usertags.HasPrevious) | $usertags.CreatePageLink( $usertags.PreviousIndex, "prev",null, "%{searchId=$!searchId,status='draft'}" )  #end
      #if(!$usertags.HasPrevious) | prev #end
      #if($usertags.HasNext) | $PaginationHelper.CreatePageLink( $usertags.NextIndex, "next",null, "%{searchId=$!searchId,status='draft'}" )  #end
      #if(!$usertags.HasNext) | next #end
      #if($usertags.HasNext) | $PaginationHelper.CreatePageLink( $usertags.LastIndex, "last",null, "%{searchId=$!searchId,status='draft'}" )  #end
      #if(!$usertags.HasNext) | last #end
    </td>
  </tr>
</table>
<table width="100%" border="1" cellpadding="2" cellspacing="0">
<tr>
    <th>Id</th>
    <th>Name</th>
    <th> </th>
</tr>

  #foreach($tag in $usertags)
  <tr>
    <td align="center">$!tag.id</td>
    <td align="center">$!tag.Name</td>
    <td align="center">
        <a href="edit_usertags.castle?id=$!{tag.id}">Edit</a> | 
        <a href="delete_usertags.castle?id=$!{tag.id}">Delete</a>
    </td>
</tr>
#end
</table>